<h1>Edit post</h1>


<form id="post-edit-form">
  <div id="errors"></div>
	
  <label>Title
  <input type="text" name="title" value="<%= post.escape('title') %>"></label>

  <label>Body
  <input type="text" name="body" value="<%= post.escape('body') %>"></label>
  
  <input type="submit" value="Save Changes"> 
</form>